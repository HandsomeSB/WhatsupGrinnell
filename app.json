{
  "expo": {
    "name": "WhatsupGrinnell",
    "jsEngine": "jsc",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#121212"
    },
    "scheme": "whatsupgrinnell",
    "ios": {
      "bundleIdentifier": "com.grinnellhackerhouse.whatsupgrinnell",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone."
      }
    },
    "plugins": [
      "expo-router",
      ["expo-audio",
      {
        "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
      }]
    ]
  }
}
