{
  "expo": {
    "name": "WhatsupGrinnell",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#121212"
    },
    "scheme": "whatsupgrinnell",
    "ios": {
      "bundleIdentifier": "com.grinnellhackerhouse.whatsupgrinnell",
      "buildNumber": "1.0.4",
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "WhatsupGrinnell needs access to your microphone to record voice search queries, transcribe your speech into text, and provide summarized responses.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-audio",
        {
          "microphonePermission": "WhatsupGrinnell needs access to your microphone to record voice search queries, transcribe speech to text, and provide summarized responses."
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "48f673ac-91ca-45d5-9fb5-58e2e2182a47"
      }
    }
  }
}
